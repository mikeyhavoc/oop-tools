{
  "name": "php-tools",
  "version": "1.0.0",
  "description": "tools site",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-clean && npm run build-sass && npm run build-autoprefix && npm run build-cleancss",
    "build-autoprefix": "postcss --use autoprefixer --map false --output css/bulma.css css/bulma.css",
    "build-clean": "rimraf css",
    "build-cleancss": "cleancss -o css/bulma.min.css css/bulma.css",
    "build-sass": "node-sass --output-style expanded --source-map true bulma.sass css/bulma.css",
    "deploy": "npm run build",
    "start": "npm run build-sass -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeyhavoc/php_tools.git"
  },
  "author": "michael williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mikeyhavoc/php_tools/issues"
  },
  "homepage": "https://github.com/mikeyhavoc/php_tools#readme",
  "devDependencies": {
    "@cedx/gulp-php-minify": "^7.1.1",
    "autoprefixer": "^9.5.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "clean-css-cli": "^4.3.0",
    "eslint": "^4.19.1",
    "fs": "0.0.1-security",
    "gulp": "^4.0.0",
    "gulp-clean-css": "^3.9.4",
    "gulp-cli": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^1.1.0",
    "gulp-debug": "^3.2.0",
    "gulp-htmlclean": "^2.7.16",
    "gulp-imageoptim": "^1.0.9",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^3.2.0",
    "gulp-rigger": "^0.5.8",
    "gulp-sass": "^4.0.1",
    "gulp-series": "^1.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-util": "^3.0.8",
    "node-sass": "^4.11.0",
    "postcss-cli": "^6.1.2",
    "rimraf": "^2.6.3"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3"
  }
}
